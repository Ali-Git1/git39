@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container">
    <h2>User Data</h2>
    <table class="table table-hover table-dark">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Name</th>
                <th scope="col">SurName</th>
                <th scope="col">Age</th>
                <th scope="col">Country</th>
                <th scope="col">PhoneNumber</th>

            </tr>
        </thead>
        <tbody>
            <tr>
                <th scope="row">1</th>
                <td>@ViewBag.Name</td>
                <td>@ViewBag.SurName</td>
                <td>@ViewBag.Age</td>
                <td>@ViewBag.Country</td>
                <td>@ViewBag.PhoneNumber</td>
            </tr>
            @* <tr>
                <th scope="row">2</th>
                <td>Jacob</td>
                <td>Thornton</td>
                <td>@fat</td>
            </tr>
            <tr>
                <th scope="row">3</th>
                <td colspan="2">Larry the Bird</td>
                <td>@twitter</td>
            </tr> *@
        </tbody>
    </table>
</div>